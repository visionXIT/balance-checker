      "sourceRoot": "src"
    },
    "schedule": {
      "type": "application",
      "root": ".",
      "entryFile": "bootstrapSchedule",
      "sourceRoot": "src"
    }
  },
  "compilerOptions": {
    "webpack": false
  }
}
